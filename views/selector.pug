extends layout

block content
  h1= title
  - let image_names = ["bear.jpg","chimpu.jpg","dog.png","panda.jpg","zebra.jpg"] 
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]
      img(src=`images/${iname}` alt='animal' style='width:300px; height:300px;')
  - }


            